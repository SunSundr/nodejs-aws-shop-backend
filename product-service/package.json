{
  "name": "product-service",
  "version": "0.1.0",
  "bin": {
    "product-service": "dist/bin/product-service.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:deploy": "npm run build && cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "npm run build && cdk diff",
    "cdk:synth": "npm run build && cdk synth",
    "seed": "npm run build && node ./dist/db/seed.js",
    "lint": "eslint . --fix",
    "prettier": "prettier lambda lib test --write"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "aws-cdk": "^2.1000.2",
    "esbuild": "^0.25.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.758.0",
    "@aws-sdk/lib-dynamodb": "^3.758.0",
    "aws-cdk-lib": "^2.179.0",
    "constructs": "^10.4.2"
  }
}
