{
  "name": "cart-service",
  "version": "0.1.0",
  "bin": {
    "cart-service": "dist/bin/cart-service.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc && cp lib/sql/*.sql dist/lib/sql",
    "watch": "tsc -w",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:deploy": "npm run build && cdk deploy --outputs-file dist/cdk-outputs.json",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "npm run build && cdk diff",
    "cdk:synth": "npm run build && cdk synth",
    "lint": "eslint . --fix",
    "prettier": "prettier . --write",
    "db:init": "node dist/lib/sql/init-db.js",
    "db:seed": "node dist/lib/sql/seed.js"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.148",
    "@types/jest": "^29.5.14",
    "@types/node": "22.14.0",
    "@types/pg": "^8.11.11",
    "aws-cdk": "2.1007.0",
    "esbuild": "^0.25.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.29.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.188.0",
    "constructs": "^10.4.2",
    "dotenv": "^16.4.7",
    "pg": "^8.14.1"
  }
}
