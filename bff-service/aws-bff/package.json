{
  "name": "aws-bff",
  "version": "0.1.0",
  "bin": {
    "aws-bff": "dist/bin/aws-bff.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "cdk:deploy": "npm run build && cdk deploy --outputs-file dist/cdk-outputs.json",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "npm run build && cdk diff",
    "cdk:synth": "npm run build && cdk synth"
  },
  "dependencies": {
    "aws-cdk-lib": "2.181.1",
    "constructs": "^10.0.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@types/node": "22.7.9",
    "aws-cdk": "2.1005.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  }
}
